generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model posts {
  post_id  Int    @id @default(autoincrement())
  username String @db.VarChar(255)
  fullname String @db.VarChar(255) 
  minute   Int
  word     String @db.VarChar(255)
}
